{
  "name": "rot-ritual-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chainlink/contracts": "^1.4.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@rot-ritual/abi": "file:../../packages/abi",
    "@rot-ritual/addresses": "file:../../packages/addresses",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.11.0",
    "canvas-confetti": "^1.9.2",
    "date-fns": "^4.1.0",
    "ethers": "^6.9.0",
    "form-data": "^4.0.4",
    "framer-motion": "^10.18.0",
    "howler": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.0",
    "react-tooltip": "^5.25.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.0",
    "eslint": "^9.34.0",
    "hardhat": "^2.19.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0"
  },
  "scripts": {
    "predev": "node ../../scripts/build-abis.mjs",
    "dev": "vite",
    "prebuild": "cd ../../ && ./scripts/check-no-old-address.sh && node apps/web/scripts/check-no-env-addresses.js",
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx",
    "check:noaddrs": "node scripts/check-no-env-addresses.js",
    "test:balances": "node scripts/test-balance-accuracy.js",
    "preview": "vite preview",
    "build:prod": "vite build --mode production && cp src/main.production.jsx dist/main.jsx",
    "node": "hardhat node",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy-core.js --network hardhat",
    "deploy:sepolia": "hardhat run scripts/deploy-local.js --network sepolia",
    "deploy:base": "hardhat run scripts/deploy-base-simple.js --network baseSepolia",
    "test:local": "npm run deploy:local && npm run dev",
    "clean": "hardhat clean"
  }
}
